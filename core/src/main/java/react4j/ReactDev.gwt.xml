<module>
  <inherits name='react4j.React'/>
  <inherits name='arez.ArezDev'/>
  <inherits name='org.realityforge.braincheck.BrainCheckDev'/>

  <source path='ignored'/>

  <public path='public' includes='dev/*'/>

  <set-property name='react4j.enable_component_names' value='true'/>
  <set-property name='react4j.check_invariants' value='true'/>
  <set-property name='react4j.minimize_prop_keys' value='false'/>
  <set-property name='react4j.validate_prop_values' value='true'/>
  <set-property name='react4j.store_debug_data_as_state' value='true'/>
  <set-property name='react4j.freeze_props' value='true'/>
</module>
